<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/appcolor_transparent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:id="@+id/inner_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="101dp"
        android:layout_centerHorizontal="true">
        <com.nebula.uikit.textview.RobotoRegularTextView
            android:textSize="14dp"
            android:textColor="@color/colorWhite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="57dp"
            android:text="@string/yr_pk_gifting_in_this_round"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="3dp">
            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginTop="2dp"
                android:src="@drawable/diamonds"/>
            <com.nebula.uikit.textview.RobotoBoldTextView
                android:textSize="14dp"
                android:textColor="@color/colorWhite"
                android:id="@+id/pk_ending_diamond_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_marginStart="2dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal"
            android:id="@+id/extra_layout"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp">
            <com.nebula.uikit.textview.RobotoRegularTextView
                android:textSize="14dp"
                android:textColor="@color/colorWhite"
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/yr_extra_reward"/>
            <ImageView
                android:id="@+id/extra_img"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="4dp"/>
            <com.nebula.uikit.textview.RobotoRegularTextView
                android:textSize="14dp"
                android:textColor="@color/colorWhite"
                android:gravity="center_vertical"
                android:id="@+id/extra_count_text"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="4dp"/>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp">
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/pk_ending_result_layout"
                android:background="@drawable/pk_ending_bottom_winner_bg"
                android:paddingTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="115dp"
                android:layout_marginTop="10dp"
                android:weightSum="3">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="6dp"
                    android:layout_weight="1">
                    <RelativeLayout
                        android:layout_width="40dp"
                        android:layout_height="45dp">
                        <com.funny.yari.ui.widget.CircleImageView
                            android:id="@+id/second_icon"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginStart="2.5dp"/>
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="0x7f0802fc"/>
                    </RelativeLayout>
                    <com.nebula.uikit.textview.RobotoRegularTextView
                        android:textSize="12dp"
                        android:textColor="@color/colorWhite"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/second_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:text="---"
                        android:maxLines="1"/>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:layout_marginTop="2dp"
                            android:src="@drawable/diamonds"/>
                        <com.nebula.uikit.textview.RobotoRegularTextView
                            android:textSize="10dp"
                            android:textColor="@color/colorWhite"
                            android:id="@+id/pk_ending_diamond_count_second"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_marginStart="2dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <RelativeLayout
                        android:layout_width="46dp"
                        android:layout_height="51dp">
                        <com.funny.yari.ui.widget.CircleImageView
                            android:id="@+id/top_icon"
                            android:layout_width="41dp"
                            android:layout_height="41dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginStart="2.5dp"/>
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/pk_ending_contributor_top"/>
                    </RelativeLayout>
                    <com.nebula.uikit.textview.RobotoRegularTextView
                        android:textSize="12dp"
                        android:textColor="@color/colorWhite"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/top_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2.5dp"
                        android:text="---"
                        android:maxLines="1"/>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:layout_marginTop="1dp"
                            android:src="@drawable/diamonds"/>
                        <com.nebula.uikit.textview.RobotoRegularTextView
                            android:textSize="10dp"
                            android:textColor="@color/colorWhite"
                            android:id="@+id/pk_ending_diamond_count_top"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_marginStart="2dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="6dp"
                    android:layout_weight="1">
                    <RelativeLayout
                        android:layout_width="40dp"
                        android:layout_height="45dp">
                        <com.funny.yari.ui.widget.CircleImageView
                            android:id="@+id/third_icon"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginStart="2.5dp"/>
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/pk_ending_contributor_third"/>
                    </RelativeLayout>
                    <com.nebula.uikit.textview.RobotoRegularTextView
                        android:textSize="12dp"
                        android:textColor="@color/colorWhite"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/third_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:text="---"
                        android:maxLines="1"/>
                    <LinearLayout
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:layout_width="14dp"
                            android:layout_height="14dp"
                            android:layout_marginTop="2dp"
                            android:src="@drawable/diamonds"/>
                        <com.nebula.uikit.textview.RobotoRegularTextView
                            android:textSize="10dp"
                            android:textColor="@color/colorWhite"
                            android:id="@+id/pk_ending_diamond_count_third"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_marginStart="2dp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <com.nebula.uikit.textview.RobotoBoldTextView
                android:textSize="14dp"
                android:textColor="@color/colorWhite"
                android:gravity="center"
                android:id="@+id/pk_ending_contribute_title"
                android:background="@drawable/pk_ending_bottom_winner_header"
                android:layout_width="140dp"
                android:layout_height="20dp"
                android:text="@string/yr_top_3_contributors"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="2dp"/>
        </RelativeLayout>
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/pk_ending_header_layout"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true">
        <ImageView
            android:id="@+id/pk_ending_winner_boom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/pk_ending_winner_header"/>
        <com.funny.yari.ui.widget.CircleImageView
            android:id="@+id/pk_ending_header_icon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:scaleType="centerCrop"
            android:layout_centerInParent="true"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/winnerLayout"
            android:background="@drawable/pk_end_winner_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="123dp"
            android:layout_centerHorizontal="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/pk_end_win"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/pk_ending_series_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/pk_end_win_x"
                    android:layout_marginStart="2dp"/>
                <ImageView
                    android:id="@+id/pk_ending_first_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="2dp"/>
                <ImageView
                    android:id="@+id/pk_ending_second_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="1dp"/>
                <ImageView
                    android:id="@+id/pk_ending_third_count"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="1dp"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    <com.nebula.uikit.textview.NenoRobotoBoldTextView
        android:textSize="14dp"
        android:textColor="#e76e0c"
        android:gravity="center"
        android:id="@+id/pk_ending_result_ok_btn"
        android:background="@drawable/pk_ending_winner_ok_btn"
        android:layout_width="167dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:text="@string/yr_big_ok"
        android:layout_below="@+id/inner_info"
        android:layout_centerHorizontal="true"/>
</RelativeLayout>